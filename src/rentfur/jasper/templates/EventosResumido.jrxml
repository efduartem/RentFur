	<queryString>
		<![CDATA[SELECT
	e.delivery_date,
	e.id,
	--e.number,
	e.contract_number,
	e.subject_code,
	e.subject_name,
	e.net_total
FROM
	event e
WHERE
	e.delivery_date >= to_timestamp( '$P!{fecha_ini_str} 00:00:00', 'DD/MM/YYYY HH24:MI:SS')
	and e.delivery_date <= to_timestamp( '$P!{fecha_fin_str} 23:59:59', 'DD/MM/YYYY HH24:MI:SS')
	and e.status = 1
order by
	e.delivery_date;]]>
	</queryString>
